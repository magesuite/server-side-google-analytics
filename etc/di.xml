<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">
    <preference for="MageSuite\ServerSideGoogleAnalytics\Model\ItemDataProviderInterface"
                type="MageSuite\ServerSideGoogleAnalytics\Model\CompositeItemDataProvider" />

    <type name="MageSuite\ServerSideGoogleAnalytics\Model\CompositeItemDataProvider">
        <arguments>
            <argument name="itemDataProviders" xsi:type="array">
                <item name="default" xsi:type="object">MageSuite\ServerSideGoogleAnalytics\Model\ItemDataProvider\General</item>
                <item name="price" xsi:type="object">MageSuite\ServerSideGoogleAnalytics\Model\ItemDataProvider\Price</item>
                <item name="brand" xsi:type="object">MageSuite\ServerSideGoogleAnalytics\Model\ItemDataProvider\Brand</item>
                <item name="quantity" xsi:type="object">MageSuite\ServerSideGoogleAnalytics\Model\ItemDataProvider\Quantity</item>
            </argument>
        </arguments>
    </type>
</config>
